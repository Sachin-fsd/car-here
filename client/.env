# React App Configuration
REACT_APP_NAME=CarHere
REACT_APP_VERSION=1.0.0
REACT_APP_ENV=development

# Backend API Configuration
REACT_APP_API_URL=http://localhost:5000
REACT_APP_SOCKET_URL=http://localhost:5000

# Supabase Configuration (Frontend Keys)
REACT_APP_SUPABASE_URL=https://your-project-id.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Stripe Configuration (Publishable Key)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Clerk Authentication (Frontend)
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Mapbox Configuration
REACT_APP_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token-here

# App URLs
REACT_APP_BASE_URL=http://localhost:3000
REACT_APP_BACKEND_URL=http://localhost:5000

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false

# Social Login Configuration
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id-here

# Payment Configuration
REACT_APP_PAYMENT_SUCCESS_URL=http://localhost:3000/payment-success
REACT_APP_PAYMENT_CANCEL_URL=http://localhost:3000/payment-cancel

# Location Services
REACT_APP_DEFAULT_LOCATION_LAT=19.076
REACT_APP_DEFAULT_LOCATION_LNG=72.8777
REACT_APP_DEFAULT_CITY=Mumbai

# App Settings
REACT_APP_MAX_UPLOAD_SIZE=10485760
REACT_APP_SUPPORTED_FILE_TYPES=image/jpeg,image/png,image/jpg
REACT_APP_DEFAULT_LANGUAGE=en

# Debug Settings
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=info

# Temporary Development Data
REACT_APP_DEMO_USER_EMAIL=demo@carhere.com
REACT_APP_DEMO_USER_PASSWORD=demo123
REACT_APP_DEMO_DRIVER_EMAIL=driver@carhere.com
REACT_APP_DEMO_DRIVER_PASSWORD=driver123

# Build Configuration
GENERATE_SOURCEMAP=true
DISABLE_ESLINT_PLUGIN=false